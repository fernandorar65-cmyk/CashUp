# Server
PORT=3000
NODE_ENV=development

# Database (PostgreSQL)
DATABASE_URL="postgresql://user:password@localhost:5432/cashup_db?schema=public"

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
